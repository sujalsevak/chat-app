{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/client.js",
      "destination": "/public/client.js"
    },
    {
      "source": "/style.css",
      "destination": "/public/style.css"
    },
    {
      "source": "/socket.io/(.*)",
      "destination": "server.js"
    },
    {
      "source": "/(.*)",
      "destination": "server.js"
    }
  ]
}